{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "mini-gen",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "description": "Library type api/app",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    },
    "command": {
      "type": "string",
      "description": "command name to add",
      "$default": {
        "$source": "argv",
        "index": 2
      }
      
    }


  },
  "required": ["type","name"]
}
